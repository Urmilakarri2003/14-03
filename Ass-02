#include<stdio.h>
main()
{
int missingNumber(int* nums, int numsSize)
{
    unsigned long long expected, total, n;
    int i;
    
	n = numsSize;
    total = 0;
	
    for (i = 0; i < numsSize; i++)
        total += (unsigned long long) nums[i];

    expected = (n * (n + 1)) / 2;
    return expected - total;
}

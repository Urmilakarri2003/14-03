#include <stdio.h>
int main()
{
    int t;
    scanf("%d", &t);
    while(t--)
    {
        int n;
        scanf("%d", &n); 
        int a[n], sum = 0;
        for(int i = 0; i < n; i++)
        {
            scanf("%d", &a[i]);
            sum += a[i]; 
        }
        for(int i = 0; i < n; i++)
        {
            printf("%d ", sum - a[i]);
        }
        printf("\n");
    }
     return 0;
}
